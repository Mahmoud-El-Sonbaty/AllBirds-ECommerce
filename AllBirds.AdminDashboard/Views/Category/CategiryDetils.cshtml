@model CUCategoryDTO;
@{
    ViewData["Title"] = "CategiryDetils";
}
@section Navbar {

    <partial name="_NavbarPartial" />
}
@section Sidebar {

    <partial name="_SidebarPartial" />
}
@section Footer {

    <partial name="_FooterPartial" />
}
<h1>CategoryDetils</h1>

<div class="row ">
    <div class="col-12 grid-margin">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">CategoryDetils</h4>
                <p class="card-description">
                    <a class="btn btn-primary" asp-controller="Category" asp-action="GetAll"><i class="mdi mdi-backburger"></i> GetAll</a>
                </p>
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th> Name En </th>
                                <th> Name Ar </th>
                                <th> Level </th>
                                <th> Parent Category name </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td> @Model.NameEn </td>
                                <td> @Model.NameAr </td>
                                <td> @ViewBag.levelName </td>
                                <td> @ViewBag.ParentCategoryname </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
