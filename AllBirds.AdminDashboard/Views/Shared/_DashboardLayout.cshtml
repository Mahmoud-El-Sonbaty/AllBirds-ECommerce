@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - Admin Dashboard</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/assets/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/assets/css/modern-vertical/style.css">
    <!-- endinject -->
    <!-- Plugin css for notifications -->
    <link rel="stylesheet" href="~/assets/vendors/jquery-toast-plugin/jquery.toast.min.css">
    <!-- End plugin css for notifications -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    <link rel="stylesheet" href="~/assets/css/style.css">
    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/assets/images/allbirdslogo-mini.svg" />
</head>
<body>
    <div class="container-scroller">
        <!-- optional section sidebar -->
        @await RenderSectionAsync("Sidebar", required: false)
        <!-- optional -->
        <!-- partial:partials/_sidebar.html -->
        @* <partial name="_SidebarPartial" /> *@
        <!-- partial -->
        <!-- page-body-wrapper -->
        <div class="container-fluid page-body-wrapper">
            <!-- optional section navbar -->
            @await RenderSectionAsync("Navbar", required: false)
            <!-- optional -->
            <!-- partial:partials/_navbar.html -->
            @* <partial name="_NavbarPartial" /> *@
            <!-- partial -->
            <!-- main-panel -->
            <div class="main-panel">
                <!-- content-wrapper -->
                <div class="content-wrapper text-white">
                    <!-- Modal starts -->
                    <div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel-3" aria-hidden="true">
                        <div class="modal-dialog modal-sm" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel-3">Attention</h5>
                                    <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">&times;</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <p id="modal-msg"></p>
                                </div>
                                <div class="modal-footer">
                                    <a type="button" id="modal-delete-btn" class="btn btn-danger">Delete</a>
                                    <button id="modal-cancel-btn" onclick="closeModal()" type="button" class="btn btn-light" data-bs-dismiss="modal">Cancel</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Modal Ends -->
                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- optional section footer -->
                @await RenderSectionAsync("Footer", required: false)
                <!-- optional -->
                <!-- partial:partials/_footer.html -->
                @* <partial name="_FooterPartial" /> *@
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/assets/js/off-canvas.js"></script>
    <script src="~/assets/js/hoverable-collapse.js"></script>
    <script src="~/assets/js/misc.js"></script>
    <script src="~/assets/js/settings.js"></script>
    <script src="~/assets/js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page -->
    <script src="~/assets/js/dashboard.js"></script>
    <!-- End custom js for this page -->
    <!-- Custom js for modal -->
    <script src="~/js/modal.js"></script>
    <!-- End custom js for modal -->
    <!-- Custom js for notifications -->
    <script src="~/assets/vendors/jquery-toast-plugin/jquery.toast.min.js"></script>
    <script src="~/assets/vendors/jquery-toast-plugin/toast.demo.js"></script>
    @* <script src="~/assets/js/desktop-notification.js"></script> *@
    <!-- End custom js for notifications -->
    <!-- Site js -->
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- End Site js -->
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>